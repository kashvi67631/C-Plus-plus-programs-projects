#include <iostream>
using namespace std;
//to move neg to left and positive to right and 0 in betwenn 
int main(){
    int arr[5]={1,2,3,4,5};int n=5,index=0;int narr[5];
    for ( int j=0;j<n;j++){
        if(arr[j]<0){
            narr[index++]=arr[j];
        }
        if(arr[j]==0){
            narr[index++]=arr[j];
        }
        if(arr[j]>0){
            narr[index++]=arr[j];
        }
    }
    for (int i = 0; i < n; i++)
    cout << arr[i] << " ";

    
}
